{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/" },
      { "src": "/api/users", "dest": "/users" },
      { "src": "/api/auth", "dest": "/auth" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }